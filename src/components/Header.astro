<style>
  header {
    margin: 0;
    padding: 0 1em;
    background: var(--base);
    box-shadow: 0 2px 8px rgba(var(--black), 5%);
    font-family: "RL Aqva";
  }
  h2 {
    margin: 0;
    font-size: 1em;
  }

  #logo {
    margin: 0;
    padding: 0;
    height: 86px;
  }

  h2 a,
  h2 a.active {
    text-decoration: none;
  }
  nav {
    display: flex;
    align-items: center;
    justify-content: space-between;
  }
  nav a {
    padding: 1em 0.5em;
    color: var(--black);
    border-bottom: 4px solid transparent;
    text-decoration: none;
  }
  nav a.active {
    text-decoration: none;
    border-bottom-color: #df0006;
  }
  .social-links,
  .social-links a {
    display: flex;
  }
  @media (max-width: 720px) {
    .social-links {
      display: none;
    }
  }
</style>
